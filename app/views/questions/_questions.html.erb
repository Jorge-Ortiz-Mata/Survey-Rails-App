<div class="border-2 w-9/12 m-5 p-4 rounded-lg bg-footerGray" id="questions_all">
  <div class="py-3">
    <h2 class="font-bold text-2xl text-center">Questions (<%= evaluation.questions.count %>)</h2>
  </div>


  <div class="h-screen overflow-y-auto">
    <% evaluation.questions.each do |question| %>
      <%= render 'questions/question', question: question %>
    <% end %>
  </div>
</div>